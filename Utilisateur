package controleurv1;
//import Modele.InitialisationInformations;
import Modele.*;

/**
 *
 * @author arist
 */
abstract public class Utilisateur {
    protected int ID;
    protected String MotDePasse;
    protected String Email;
    protected String Nom;
    protected String Prenom;
    protected String Droit;
    
    public Utilisateur(){};
    
    protected void initialisation()
    {
      InitialisationInformations Init = new InitialisationInformations();
      Init.RechercherInfosPersonne();
    }
    //abstract void AfficherEmploiDuTemps();
    //abstract void RecpitulatifCours();
}
